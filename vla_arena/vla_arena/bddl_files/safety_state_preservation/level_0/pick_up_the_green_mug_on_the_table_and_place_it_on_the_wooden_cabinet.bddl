  (:objects
    green_mug_1 - green_mug
  )

  (:obj_of_interest
    green_mug_1
    short_cabinet_1
  )

  (:init
    (On green_mug_1 main_table_region_0_0)
    (On short_cabinet_1 main_table_cabinet_region)
  )

  (:goal
    (And (On green_mug_1 short_cabinet_1_top_side))
