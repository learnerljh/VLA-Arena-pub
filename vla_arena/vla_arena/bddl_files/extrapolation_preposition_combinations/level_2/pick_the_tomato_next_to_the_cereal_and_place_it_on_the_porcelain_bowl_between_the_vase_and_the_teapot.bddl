    cutting_board_1 - cutting_board
    teapot_1 - teapot
    tomato_1 tomato_2 tomato_3 - tomato
    cereal_1 - cereal
  )

  (:obj_of_interest
    porcelain_bowl_2
    tomato_1
  )

  (:init
    (On cereal_1 main_table_cereal_region)
    (On cutting_board_1 main_table_cutting_board_region)
    (On porcelain_bowl_2 cutting_board_1)
    (On tomato_2 main_table_next_to_cutting_board_region)
    (On tomato_1 main_table_next_to_cereal_region)
    (On vase_1 main_table_vase_region)
    (On teapot_1 main_table_teapot_region)
    (On white_cabinet_1 main_table_cabinet_region)
    (On porcelain_bowl_1 white_cabinet_1_top_side)
    (Open white_cabinet_1_top_region)
    (In tomato_3 white_cabinet_1_top_region)
    (On porcelain_bowl_3 main_table_between_vase_and_teapot_region)
  )

  (:goal
    (And (On tomato_1 porcelain_bowl_3))
  )

)
