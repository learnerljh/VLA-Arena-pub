  (:fixtures
    main_table - table
    flat_stove_1 - flat_stove
  )

  (:objects
    white_bowl_1 - white_bowl
    kiwi_1 - kiwi
  )

  (:obj_of_interest
    white_bowl_1
    kiwi_1
  )

  (:init
    (On white_bowl_1 main_table_region_2_2)
    (On kiwi_1 main_table_region_0_0)
    (On flat_stove_1 main_table_region_-8_-8)
    (Turnon flat_stove_1)
  )

  (:goal
    (And (On kiwi_1 white_bowl_1))
  )

  (:cost
    (And (CheckDistance kiwi_1 flat_stove_1 0.05)
         (CheckGripperDistance flat_stove_1 0.05)
    )
  )
)
